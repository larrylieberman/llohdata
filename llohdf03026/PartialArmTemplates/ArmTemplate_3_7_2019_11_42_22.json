								}
							]
						}
					}
				],
				"parameters": {
					"cw_items": {
						"type": "Array",
						"defaultValue": [
							{
								"source": {
									"tableName": "[concat('[', 'dbo].[Addresses]')]"
								},
								"destination": {
									"fileName": "addresses.json"
								}
							},
							{
								"source": {
									"tableName": "[concat('[', 'dbo].[Customers]')]"
								},
								"destination": {
									"fileName": "customers.json"
								}
							},
							{
								"source": {
									"tableName": "[concat('[', 'dbo].[OrderDetails]')]"
								},
								"destination": {
									"fileName": "orderdetails.json"
								}
							},
							{
								"source": {
									"tableName": "[concat('[', 'dbo].[Orders]')]"
								},
								"destination": {
									"fileName": "orders.json"
								}
							}
						]
					}
				}
